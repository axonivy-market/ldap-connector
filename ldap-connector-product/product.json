{
	"installers": [
		{
			"id": "maven-import",
			"data": {
				"projects": [
					{
						"groupId": "com.axonivy.connector.ldap",
						"artifactId": "${project.name}-demo",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories": [
					{
						"id": "axonivy.public",
						"url": "https://maven.axonivy.com",
						"snapshots": {
							"enabled": "true"
						}
					}
				]
			}
		},
		{
			"id": "maven-dependency",
			"data": {
				"dependencies": [
					{
						"groupId": "com.axonivy.connector.ldap",
						"artifactId": "${project.name}",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories": [
					{
						"id": "axonivy.public",
						"url": "https://maven.axonivy.com",
						"snapshots": {
							"enabled": "true"
						}
					}
				]
			}
		}
	]
}
